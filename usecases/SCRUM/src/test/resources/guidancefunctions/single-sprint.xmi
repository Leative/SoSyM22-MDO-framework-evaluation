    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:planning="http://www.kcl.ac.uk/mdeoptimiser/sprint-planning"
    minSprints="0"
    maxSprints="10"
    currentSprints="1">
  <stakeholders
      workitem="//@backlog/@workitems.0"/>
  <stakeholders
      workitem="//@backlog/@workitems.1"/>
  <backlog>
    <workitems
        isPlannedFor="//@sprints.0"
        Importance="6"
        Effort="6"
        stakeholder="//@stakeholders.0"/>
    <workitems
        isPlannedFor="//@sprints.0"
        Importance="10"
        Effort="10"
        stakeholder="//@stakeholders.1"/>
  </backlog>
  <sprints committedItem="//@backlog/@workitems.1 //@backlog/@workitems.0"/>
</planning:Plan>
