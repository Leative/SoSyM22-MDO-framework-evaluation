<?xml version="1.0" encoding="UTF-8"?>
<planning:Plan
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:planning="http://www.kcl.ac.uk/mdeoptimiser/sprint-planning"
    maxSprints="1"
    minSprints="1"
    currentSprints="1">
  <stakeholders
      workitem="//@backlog/@workitems.0 //@backlog/@workitems.1"/>
  <backlog>
    <workitems
        isPlannedFor="//@sprints.0"
        Importance="1"
        Effort="1"
        stakeholder="//@stakeholders.0"/>
    <workitems
        isPlannedFor="//@sprints.0"
        Importance="2"
        Effort="2"
        stakeholder="//@stakeholders.0"/>
  </backlog>
  <sprints committedItem="//@backlog/@workitems.0 //@backlog/@workitems.1"/>
</planning:Plan>
